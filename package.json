{
  "name": "theme-builder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@native-elements/core": "0.17.5",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "@types/jest": "24.0.0",
    "@types/node": "12.0.0",
    "@types/react": "16.9.0",
    "@types/react-dom": "16.9.0",
    "clsx": "1.1.0",
    "cssnano": "4.1.10",
    "cssnano-preset-advanced": "4.0.7",
    "formik": "2.1.4",
    "postcss-easy-import": "3.0.0",
    "postcss-normalize": "9.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-reporter": "6.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-intl": "4.5.0",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.1",
    "react-use": "14.1.1",
    "redux": "4.0.5",
    "redux-aar": "1.0.1",
    "redux-devtools-extension": "2.13.8",
    "redux-persist": "6.0.0",
    "tiinvo": "1.6.4",
    "typescript": "3.7.2",
    "yup": "0.28.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "translate:clean": "rimraf ./src/extracted",
    "translate:compile": "tsc -p . --target ES6 --module esnext --jsx preserve --outDir src/extracted --noEmit false",
    "translate:extract": "babel ./src/extracted",
    "translate:manage": "node ./manageTranslations.js",
    "translate:mkdirp": "mkdirp ./src/extracted",
    "translate": "npm run translate:mkdirp && npm run translate:compile && npm run translate:extract && npm run translate:manage && npm run translate:clean"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.css": "stylelint"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "7.9.4",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.4",
    "autoprefixer": "9.7.6",
    "babel-plugin-react-intl": "7.5.1",
    "husky": "4.2.5",
    "lint-staged": "10.1.7",
    "prettier": "2.0.5",
    "react-app-rewire-postcss": "3.0.2",
    "react-app-rewired": "2.1.5",
    "react-intl-translations-manager": "5.0.3",
    "stylelint": "13.3.3",
    "stylelint-config-oval": "1.0.6"
  }
}

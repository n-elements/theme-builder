{
  "name": "theme-builder",
  "version": "1.1.3",
  "private": true,
  "contributors": [
    "Mattia Astorino <astorino.design@gmail.com> (https://equinusocio.dev)",
    "Paolo Roth <iamoctod@gmail.com> (https://iamoctod.com)"
  ],
  "dependencies": {
    "@native-elements/core": "1.2.3",
    "@testing-library/jest-dom": "5.11.0",
    "@testing-library/react": "10.4.3",
    "@testing-library/user-event": "12.0.11",
    "@types/jest": "26.0.3",
    "@types/node": "14.0.14",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "clsx": "1.1.1",
    "color": "3.1.2",
    "css-color-converter": "1.1.1",
    "cssnano": "4.1.10",
    "cssnano-preset-advanced": "4.0.7",
    "formik": "2.1.4",
    "framer-motion": "1.11.1",
    "postcss-color-mod-function": "3.0.3",
    "postcss-dark-theme-class": "0.4.0",
    "postcss-easy-import": "3.0.0",
    "postcss-inset": "1.0.0",
    "postcss-normalize": "9.0.0",
    "postcss-preset-env": "6.7.0",
    "re-resizable": "6.5.1",
    "react": "16.13.1",
    "react-color": "2.18.1",
    "react-dom": "16.13.1",
    "react-frame-component": "4.1.2",
    "react-helmet": "6.1.0",
    "react-intl": "4.7.5",
    "react-loading-skeleton": "2.1.1",
    "react-modal": "3.11.2",
    "react-redux": "7.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "react-use": "15.3.2",
    "redux": "4.0.5",
    "redux-aar": "1.0.1",
    "redux-devtools-extension": "2.13.8",
    "redux-persist": "6.0.0",
    "tiinvo": "1.8.0",
    "typescript": "3.9.5",
    "what-input": "5.2.10",
    "yup": "0.29.1"
  },
  "scripts": {
    "prestart": "npm run generate:icons",
    "start": "react-app-rewired start",
    "prebuild": "npm run generate:icons",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "generate:icons": "svgr --typescript --config-file svgr.config.js -d src/components/Icons/ assets/icons",
    "translate:clean": "rimraf ./src/extracted",
    "translate:compile": "tsc -p . --target ES6 --module esnext --jsx preserve --outDir src/extracted --noEmit false",
    "translate:extract": "babel ./src/extracted",
    "translate:manage": "node ./manageTranslations.js",
    "translate:mkdirp": "mkdirp ./src/extracted",
    "translate": "npm run translate:mkdirp && npm run translate:compile && npm run translate:extract && npm run translate:manage && npm run translate:clean",
    "update:deps": "yarn upgrade-interactive --latest && yarn upgrade",
    "update:cssdb": "npx browserslist@latest --update-db"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.css": "stylelint"
  },
  "devDependencies": {
    "@babel/cli": "7.10.4",
    "@babel/core": "7.10.4",
    "@babel/helper-call-delegate": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@svgr/cli": "5.4.0",
    "@types/classnames": "2.2.10",
    "@types/color": "3.0.1",
    "@types/react-color": "3.0.4",
    "@types/react-frame-component": "4.1.1",
    "@types/react-helmet": "6.0.0",
    "@types/react-modal": "3.10.6",
    "@types/react-redux": "7.1.9",
    "@types/react-router-dom": "5.1.5",
    "autoprefixer": "9.8.4",
    "babel-plugin-react-intl": "7.5.24",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "react-app-rewire-postcss": "3.0.2",
    "react-app-rewired": "2.1.6",
    "react-intl-translations-manager": "5.0.3",
    "stylelint": "13.6.1",
    "stylelint-config-oval": "1.0.6"
  }
}
